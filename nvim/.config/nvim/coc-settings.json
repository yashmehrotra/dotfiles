{
    "languageserver": {
        "golang": {
            "command": "gopls",
            "rootPatterns": [
                "go.mod",
                ".git/",
                ".hg/"
            ],
            "filetypes": [
                "go"
            ]
        },
        "terraform": {
            "command": "terraform-lsp",
            "filetypes": [
                "terraform",
                "tf"
            ],
            "initializationOptions": {},
            "settings": {}
        }
    },
    "coc.preferences.formatOnSaveFiletypes": [
        "python",
        "go"
    ]
}
